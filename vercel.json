{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/icons/(.*)",
            "dest": "/icons/$1"
        },
        {
            "src": "/intro/(.*)",
            "dest": "/intro/$1"
        },
        {
            "src": "/creations/(.*)",
            "dest": "/creations/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}